<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">



    <TextView
        android:id="@+id/pairing_cod"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="50dp"


        android:fontFamily="@font/gilroyextrabold"
        android:text="@string/twof"

        android:textAlignment="center"

        android:textAppearance="@style/pairing_cod"
        android:textSize="@dimen/appley_filter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        android:id="@+id/container_setup_auth"
        app:layout_constraintTop_toBottomOf="@id/pairing_cod"
        >
        <View
            android:id="@+id/authboxcode"
            android:layout_width="330dp"
            android:layout_height="60.56dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="76dp"
            android:background="@drawable/auth_rect"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/enter_the_p" />


        <TextView
            android:id="@+id/enter_the_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/gilroym"
            android:gravity="center_horizontal|top"
            android:text="@string/googleauth"
            android:textAppearance="@style/enter_the_p"
            android:textSize="@dimen/connectedpair"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pairing_cod" />

        <View
            android:id="@+id/authboxsq"
            android:layout_width="39dp"
            android:layout_height="39dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="18dp"
            android:background="@drawable/auth_sq"
            app:layout_constraintBottom_toBottomOf="@+id/authboxcode"
            app:layout_constraintEnd_toEndOf="@+id/authboxcode"
            app:layout_constraintTop_toTopOf="@+id/authboxcode" />
        <View
            android:id="@+id/group234"
            android:layout_width="24dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:background="@drawable/auth_copy"
            app:layout_constraintBottom_toBottomOf="@+id/authboxsq"
            app:layout_constraintEnd_toEndOf="@+id/authboxsq"
            app:layout_constraintStart_toStartOf="@+id/authboxsq"
            app:layout_constraintTop_toTopOf="@+id/authboxsq"
            app:layout_constraintVertical_bias="0.47000003" />

        <TextView
            android:id="@+id/secret_key"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="top"
            android:fontFamily="@font/klartextmono"
            android:text="ZHZJRHH56OIAQXAIHF6AJBDW4KUSIULK"
            android:maxWidth="250dp"
            android:textAppearance="@style/h4b2h4k6vb3"
            app:layout_constraintBottom_toBottomOf="@+id/authboxcode"
            app:layout_constraintEnd_toStartOf="@+id/authboxsq"
            app:layout_constraintStart_toStartOf="@+id/authboxcode"
            app:layout_constraintTop_toTopOf="@+id/authboxcode" />


        <View
            android:id="@+id/arrow_2"
            android:layout_width="30dp"
            android:layout_height="90dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="60dp"
            android:background="@drawable/auth_arrow"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/authboxcode" />


    </androidx.constraintlayout.widget.ConstraintLayout>






    <EditText
        android:id="@+id/rectanglesdflkn_1"
        android:layout_width="330dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:hint="@string/entergen"
        android:textColor="#000"
        android:inputType="number"
        android:maxLength="6"
        android:textColorHint="#50000000"
        android:paddingLeft="40dp"
        android:fontFamily="@font/gilroym"



        android:layout_marginTop="64dp"
        android:background="@drawable/twofacode"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/container_setup_auth"
        />

    <View
        android:id="@+id/continue_btn"
        android:layout_width="0dp"
        android:layout_height="73dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/rectangle_9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />
    <View
        android:id="@+id/icons8_logi"
        android:layout_width="0dp"
        android:layout_height="37.22dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/whitearrowicon"
        app:layout_constraintBottom_toBottomOf="@+id/continue_btn"
        app:layout_constraintEnd_toEndOf="@+id/continue_btn"
        app:layout_constraintStart_toEndOf="@+id/continueword"
        app:layout_constraintTop_toTopOf="@+id/continue_btn" />
    <TextView
        android:id="@+id/continueword"
        android:layout_width="79dp"
        android:layout_height="25.12dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:textSize="@dimen/SendtoWeb"
        android:textAlignment="center"
        android:fontFamily="@font/gilroyb"
        android:text="@string/continue1"
        android:textAppearance="@style/continue1"
        app:layout_constraintBottom_toBottomOf="@+id/continue_btn"
        app:layout_constraintEnd_toEndOf="@+id/continue_btn"
        app:layout_constraintStart_toStartOf="@+id/continue_btn"
        app:layout_constraintTop_toTopOf="@+id/continue_btn" />


</androidx.constraintlayout.widget.ConstraintLayout>