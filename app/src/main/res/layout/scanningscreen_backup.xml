<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".MainActivity">


    <View
        android:id="@+id/rectangle_3"
        android:layout_width="96dp"
        android:layout_height="34dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/round_red"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/disconnect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"


        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:fontFamily="@font/gilroyb"
        android:gravity="top"
        android:text="@string/disconnect"
        android:textAppearance="@style/disconnect"
        android:textSize="@dimen/disConnected"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_3"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="@+id/rectangle_3"
        app:layout_constraintTop_toTopOf="@+id/rectangle_3"
        tools:ignore="HardcodedText" />


    <TextView
        android:id="@+id/connected_t"
        android:layout_width="141dp"
        android:layout_height="23dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/gilroym"
        android:gravity="top"
        android:text="@string/connected_t"
        android:textAppearance="@style/connected_t"
        android:textSize="@dimen/Connected"
        app:layout_constraintBottom_toTopOf="@+id/some_id"
        app:layout_constraintEnd_toEndOf="@+id/some_id"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/some_id"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/some_id"
        android:layout_width="181dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="660dp"
        android:fontFamily="@font/cdsemi"
        android:gravity="top"
        android:text="192.168.0.1"
        android:textAppearance="@style/some_id"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/rectangle_3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText" />

    <View
        android:id="@+id/group_3"
        android:layout_width="0dp"
        android:layout_height="73dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/yellowround"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.57"
        app:layout_constraintStart_toEndOf="@+id/ellipse_1" />

    <View
        android:id="@+id/icons8_go_b"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/img_8"
        android:rotationY="180"
        app:layout_constraintBottom_toBottomOf="@+id/group_3"
        app:layout_constraintEnd_toEndOf="@+id/group_3"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="@id/group_3"
        app:layout_constraintTop_toTopOf="@id/group_3" />

    <View
        android:id="@+id/ellipse_1"
        android:layout_width="75dp"
        android:layout_height="73dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/elipse_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/icons8_came"
        android:layout_width="48dp"
        android:layout_height="52dp"
        android:background="@drawable/img_2"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse_1"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_1"
        app:layout_constraintStart_toStartOf="@+id/ellipse_1"
        app:layout_constraintTop_toTopOf="@+id/ellipse_1" />


</androidx.constraintlayout.widget.ConstraintLayout>